def main():

    print("the largest element is ",largestElement())


def largestElement():

    arr = [12,34,56,10,3,4,89,21,31,52,78,88]

    largest_element = arr[0]
    

    for element in arr:
        if element > largest_element:
            largest_element = element

    return largest_element        

if __name__ == "__main__":
    main()